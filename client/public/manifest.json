{
  "manifest_version": 3,
  "name": "FIO Clone",
  "version": "1.0.0",
  "description": "A privacy-preserving, local-first time zone planner and new tab page.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage"],
  "icons": {
    "128": "favicon.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
